<div class="page-width" data-aos="fade-up" data-aos-duration="2000">
   <div style="margin:0 auto;margin-top: -200px;" id="customform_data" data-id="a2759e8898efe15e8a7a01"></div>
</div>
<div class="page-width">
  <div class="content-block">
    <div class="rte rte--indented-images">
      {{ page.content }}
    </div>
  </div>

  
</div>

<img src="https://cdn.shopify.com/s/files/1/0038/3071/0342/files/line-prefoter.png" class="pre-footer">

  <div class="section-block bg-blue section-block--padding">
    <div class="{% if section.settings.heading != blank or section.settings.subheading != blank %}section-block__header {% endif %}text-center">
        <h4 class="h1--mini section-block__title" style="color: #fff;">SUBSCRIBE TO OUR NEWSLETTER</h4>
        <div class="section-block__subtext" style="color: rgba(205, 220, 236, 0.8);margin-bottom: 30px;">Promotions, new products and sales. Directly to your inbox.</div>
    </div>
    <div class="grid">
      <div class="grid__item medium-up--two-thirds medium-up--push-one-sixth">
        <div class="form-vertical">
          {% form 'customer' %}
            {{ form.errors | default_errors }}
            {% if form.posted_successfully? %}
              <p class="form-success">{{ 'general.newsletter_form.confirmation' | t }}</p>
            {% else %}
              <input type="hidden" name="contact[tags]" value="newsletter">
              <label for="NewsletterEmail-{{ block.id }}" class="label--hidden">{{ 'general.newsletter_form.newsletter_label' | t }}</label>
              <div class="input-group">
                <input type="email" value="{% if customer %}{{ customer.email }}{% endif %}" placeholder="{{ 'general.newsletter_form.newsletter_placeholder' | t }}" name="contact[email]" id="NewsletterEmail-{{ block.id }}" class="input-group__field" autocorrect="off" autocapitalize="off">
                <div class="input-group__btn">
                  <button type="submit" class="btn btn--narrow" name="commit">
                    {% include 'icon-arrow-right' %}
                    <span class="icon__fallback-text">{{ 'general.newsletter_form.submit' | t }}</span>
                  </button>
                </div>
              </div>
            {% endif %}
          {% endform %}
        </div>
      </div>
    </div>
  </div>

<style>
  .main-content {
    margin-top: 0px;
    margin-bottom: 0px;
    padding-bottom: 0px;
  }
  a {
    color: rgba(255,255,255,0.6);
  }
  a:hover {
    color: #fff;
  }
  .contact-social-icons {
    display: table;
    margin: 0 auto;
  }

  .contact-social-icons li {
    display: table-cell;   
  }

  .contact-social-icons li a {
    box-shadow: 4px 7px 26px rgba(0, 0, 0, 0.03);
    background-color: #fff;
    border-radius: 50px;
    padding: 10px;
    margin-right: 6px;
  }
  .content-block {
    margin-bottom: 0px;
  }
</style>